zone "tst.hcinfotech.ch" IN {
  type primary;
  file "/etc/bind/tst.hcinfotech.ch.zone";
};

// zone file for the reverse lookup
zone "50.1.10.in-addr.arpa" {
  type primary;
  file "/etc/bind/10.1.50.rev.zone";
};

