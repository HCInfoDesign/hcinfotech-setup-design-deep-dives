zone "a.example.com" IN {
  type primary;
  file "/etc/bind/a.example.com";
  allow-transfer {
    192.168.1.30;
    192.168.1.68;
  };
};

zone "1.168.192.in-addr.arpa" IN {
  type primary;
  file "/etc/bind/1.168.192.in-addr.arpa.zone";
  allow-transfer {
    192.168.1.30;
    192.168.1.68;
  };
};
